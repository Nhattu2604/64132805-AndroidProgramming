<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">


    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="431dp"
        android:layout_height="291dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo50nam" />

    <Button
        android:id="@+id/btnMH2"
        android:layout_width="108dp"
        android:layout_height="60dp"
        android:layout_marginStart="82dp"
        android:layout_marginTop="84dp"
        android:layout_marginEnd="10dp"
        android:text="Chức năng 2"
        app:layout_constraintEnd_toStartOf="@+id/btnMH3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btnMH3"
        android:layout_width="108dp"
        android:layout_height="60dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="84dp"
        android:layout_marginEnd="64dp"
        android:text="Chức năng 3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/btnMH2"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btnMH4"
        android:layout_width="108dp"
        android:layout_height="60dp"
        android:layout_marginStart="82dp"
        android:layout_marginTop="31dp"
        android:layout_marginEnd="10dp"
        android:text="Chức năng 4"
        app:layout_constraintEnd_toStartOf="@+id/btnMH5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnMH2"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btnMH5"
        android:layout_width="108dp"
        android:layout_height="60dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="31dp"
        android:layout_marginEnd="68dp"
        android:text="About Me"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/btnMH4"
        app:layout_constraintTop_toBottomOf="@+id/btnMH3"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btnMH6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="162dp"
        android:layout_marginTop="34dp"
        android:layout_marginEnd="161dp"
        android:text="bonus"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnMH4" />
</androidx.constraintlayout.widget.ConstraintLayout>